//Useful functions to block  some exploits or freeze some props
GM.MapPostEntity = {}

GM.MapPostEntity["slender_forest"] = function()
	local ent = ents.Create("prop_dynamic_override")
	if ent:IsValid() then
		ent:SetPos(Vector(-1072.2368164063,-5099.5903320313,52.307594299316))
		ent:SetAngles(Angle(-0.05654776468873,93.855895996094,-0.004730224609375))
		ent:SetModel(Model("models/props_c17/fence01b.mdl"))
		ent:SetKeyValue("solid", "6")
		ent:Spawn()
	end
	
	//timer.Simple(2, function()
		for k,v in pairs(ents.GetAll()) do
			if v:GetClass() == "prop_physics" then
				local phys = v:GetPhysicsObject()
				if IsValid(phys) then
					phys:EnableMotion( false ) 
				end
			end
		end
	//end)
end

GM.MapPostEntity["ttt_slender_v2"] = function()	
	timer.Simple(2, function()
		for k,v in pairs(ents.GetAll()) do
			if v:GetClass() == "prop_physics" then
				local phys = v:GetPhysicsObject()
				if IsValid(phys) then
					phys:EnableMotion( false ) 
				end
			end
		end
	end)
	for k,v in pairs(ents.FindByClass("light")) do 
		v:Fire("TurnOff","") 
	end

end

GM.MapPostEntity["cs_office"] = function()	
	timer.Simple(2, function()
		for k,v in pairs(ents.GetAll()) do
			if v:GetClass() == "prop_physics" then
				local phys = v:GetPhysicsObject()
				if IsValid(phys) then
					phys:EnableMotion( false ) 
				end
			end
		end
	end)
	for k,v in pairs(ents.FindByClass("light")) do 
		v:Fire("TurnOff","") 
	end
	
	for k,v in pairs(ents.FindByClass("light_environment")) do
		print"Found Light"
	end
	
	for k,v in pairs(ents.FindByClass("light_spot")) do 
		v:Fire("TurnOff","") 
	end
	
	for k,v in pairs(ents.FindByClass("light_dynamic")) do 
		v:Fire("TurnOff","") 
	end
	
end


GM.MapPostEntity["zs_clav_segments_v2"] = function()	
	for k,v in pairs(ents.GetAll()) do
		if v:GetClass() == "prop_physics" then
			local phys = v:GetPhysicsObject()
			if IsValid(phys) then
				phys:EnableMotion( false ) 
			end
		end
	end
	for k,v in pairs(ents.FindByClass("func_breakable")) do 
		v:Fire("break","",0) 
	end

end

GM.MapPostEntity["zs_last_mansion_r"] = function()	
	timer.Simple(1, function()
		for k,v in pairs(ents.GetAll()) do
			if v:GetClass() == "prop_physics" then
				local phys = v:GetPhysicsObject()
				if IsValid(phys) then
					phys:EnableMotion( false ) 
				end
			end
		end
	end)
	for k,v in pairs(ents.FindByClass("light")) do 
		v:Fire("TurnOff","") 
	end

end

